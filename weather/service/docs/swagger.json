            "get": {
                "description": "Получить детальный прогноз",
                "produces": [
                    "application/json"
                ],
                "summary": "Детальный прогноз",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Название города",
                        "name": "city",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Дата прогноза",
                        "name": "date",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
